{
    "expo": {
        "name": "Min Thein Kha",
        "slug": "min-thein-kha",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#5c281d"
        },
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/adaptive-icon.png",
                "backgroundColor": "#5c281d"
            }
        },
        "plugins": ["expo-router"],
        "experiments": {
            "typedRoutes": true
        }
    }
}
